    "@react-native-firebase/app": "^11.4.1",
    "@react-native-firebase/auth": "^11.4.1",
    "@react-native-picker/picker": "^1.16.6",
    "@react-navigation/devtools": "^6.0.1",
    "@react-navigation/material-top-tabs": "^6.0.2",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-navigation/stack": "^6.0.7",
    "@sentry/react-native": "2.4.2",
    "@types/node": "^17.0.6",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.9",
    "@types/uuid": "^8.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "^8.3.0",
    "date-fns": "^2.23.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expo": "^44.0.6",
    "expo-font": "^10.0.4",
    "expo-linear-gradient": "^11.0.0",
    "expo-localization": "^12.0.0",
    "expo-updates": "^0.11.3",
    "graphql-request": "^3.4.0",
    "i18n-js": "3.8.0",
    "mobx": "6.0.4",
    "mobx-react-lite": "3.1.6",
    "mobx-state-tree": "4.0.2",
    "moti": "^0.11.0",
    "mst-gql": "^0.14.0",
    "neverthrow": "^4.3.1",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-freeze": "^1.0.0",
    "react-hook-form": "^7.17.4",
    "react-native": "0.65.1",
    "react-native-auto-height-image": "^3.2.4",
    "react-native-bootsplash": "^3.2.4",
    "react-native-bouncy-checkbox": "^2.1.2",
    "react-native-collapsible": "^1.5.3",
    "react-native-deck-swiper": "^2.0.5",
    "react-native-dotenv": "^2.5.5",
    "react-native-fbsdk-next": "^4.3.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "^1.7.0",
    "react-native-in-app-review": "^3.2.2",
    "react-native-keychain": "6.2.0",
    "react-native-modal": "^13.0.0",
    "react-native-pager-view": "^5.4.4",
    "react-native-reanimated": "2.2.1",
    "react-native-redash": "^16.2.2",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "3.9.0",
    "react-native-share": "^7.0.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1-0",
    "react-native-tab-view": "^3.1.1",
    "react-native-toast-message": "^1.4.9",
    "react-native-ui-lib": "6.1.0",
    "react-native-vector-icons": "^8.1.0",
    "reactotron-mst": "3.1.3",
    "reactotron-react-native": "5.0.0",
    "use-debounce": "^7.0.0",
    "uuid": "^8.3.2",
    "validate.js": "0.13.1",
    "zod": "^3.10.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-optional-catch-binding": "7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@sentry/webpack-plugin": "^1.15.0",
    "@storybook/addon-storyshots": "6.1.10",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "5.3.23",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/detox": "^17.14.3",
    "@types/faker": "^5.5.3",
    "@types/i18n-js": "3.0.3",
    "@types/jest": "^27.0.2",
    "@types/ramda": "0.27.32",
    "@types/react": "16.14.0",
    "@types/react-native": "0.63.40",
    "@types/react-test-renderer": "16.9.4",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-jest": "^27.4.5",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "bufferutil": "4.0.2",
    "canvas": "2.6.1",
    "detox": "^18.23.1",
    "detox-recorder": "^1.0.151",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-native": "3.10.0",
    "faker": "^5.5.3",
    "husky": ">=6",
    "jest": "^27.4.7",
    "jest-circus": "25.5.4",
    "jest-date-mock": "^1.0.8",
    "jest-expo": "^44.0.1",
    "jetifier": "1.6.6",
    "lint-staged": ">=10",
    "metro-react-native-babel-preset": "0.66.0",
    "mocha": "8.2.1",
    "msw": "^0.36.3",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "^2.5.1",
    "react-devtools-core": "4.10.1",
    "react-dom": "16.14.0",
    "react-native-codegen": "^0.0.7",
    "react-native-flipper": "0.110.0",
    "react-native-web": "0.13.18",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "solidarity": "2.3.1",
    "testdouble": "^3.16.3",
    "ts-jest": "^27.0.7",
    "tsc-files": "^1.1.2",
    "typescript": "^4.4.4",
    "utf-8-validate": "5.0.3"
  },
  "jest": {
    "preset": "jest-expo",
    "clearMocks": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js",
      "test/(.*)": "<rootDir>/test/$1"
    },
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/test/setup/jest.setup.ts"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "jest-date-mock",
      "<rootDir>/test/setup/jest.setup-after-env.ts"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation|@storybook|@react-native-community|expo-localization|@unimodules|@sentry/react-native|@invertase/react-native-apple-authentication|expo-linear-gradient|expo-updates|react-native-vector-icons|@expo|expo-*)"
    ]
  },
  "resolutions": {
    "react-native/@jest/create-cache-key-function": "^27.0.2"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "detox": {
    "preset": "ts-jest",
    "test-runner": "jest",
    "initGlobals": false,
    "artifacts": {
      "rootDir": ".artifacts",
      "plugins": {
        "instruments": {
          "enabled": false
        },
        "log": {
          "enabled": true
        },
        "uiHierarchy": "enabled",
        "screenshot": {
          "enabled": true,
          "shouldTakeAutomaticSnapshots": true,
          "keepOnlyFailedTestsArtifacts": true,
          "takeWhen": {
            "testStart": true,
            "testDone": true
          }
        },
        "video": {
          "android": {
            "bitRate": 4000000
          },
          "simulator": {
            "codec": "hevc"
          }
        }
      }
    },
    "devices": {
      "emulator": {
        "type": "android.emulator",
        "device": {
          "avdName": "android_30_avd"
        }
      },
      "simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 13 Pro Max"
        }
      }
    },
    "apps": {
      "android.debug": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug app:assembleAndroidTest -DtestBuildType=debug && cd .."
      },
      "android.release": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease app:assembleAndroidTest -DtestBuildType=release && cd .."
      },
      "ios.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/punchline.app",
        "type": "ios.app",
        "build": "xcodebuild -workspace ios/punchline.xcworkspace -scheme punchline -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 12'"
      },
      "ios.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/punchline.app",
        "type": "ios.app",
        "build": "xcodebuild -workspace ios/punchline.xcworkspace -scheme punchline -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 12'"
      }
    },
    "configurations": {
      "android.emu.debug": {
        "app": "android.debug",
        "device": "emulator"
      },
      "android.emu.release": {
        "app": "android.release",
        "device": "emulator"
      },
      "ios.sim.debug": {
        "device": "simulator",
        "app": "ios.debug"
      },
      "ios.sim.release": {
        "device": "simulator",
        "app": "ios.release"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard"
    ],
    "plugins": [
      "react",
      "react-native",
      "@typescript-eslint",
      "jest"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "jest/no-focused-tests": "error",
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "indent": "off",
      "@typescript-eslint/member-delimiter-style": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/no-unnecessary-condition": [
        2
      ],
      "comma-dangle": 0,
      "multiline-ternary": 0,
      "no-undef": 0,
      "no-unused-vars": 0,
      "no-use-before-define": "off",
      "quotes": 0,
      "react-native/no-raw-text": 0,
      "react/no-unescaped-entities": 0,
      "react/prop-types": "off",
      "space-before-function-paren": 0,
      "generator-star-spacing": "off",
      "no-restricted-imports": [
        "error",
        {
          "paths": [
            {
              "name": "react-native",
              "importNames": [
                "View",
                "ViewProps",
                "Text",
                "TextProps",
                "Image",
                "ImageProps",
                "Button",
                "ButtonProps",
                "Switch",
                "TextInput",
                "Modal"
              ],
              "message": "Prefer react-native-ui-lib imports instead"
            }
          ]
        }
      ]
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "tsc-files --noEmit",
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.format:*": "prettier --write"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-file-system"
      ]
    }
  }
}
