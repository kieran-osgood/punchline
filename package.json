{
  "name": "punchline",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "test:e2e": "detox test -c ios.sim.debug",
    "build:e2e": "detox build -c ios.sim.debug",
    "record:e2e": "detox recorder --configuration \"ios.sim.release\" --outputTestFile \"~/e2e/recorded.js\" --testName \"My Recorded Test\" --record",
    "a": "detox recorder --bundleId \"com.ko.puchline\" --simulatorId booted --outputTestFile \"~/Desktop/RecordedTest.js\" --testName \"My Recorded Test\" --record",
    "ci:test:jest": "jest",
    "ci:build:e2e": "detox build -c ios.sim.release",
    "ci:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:e2e:android": "detox build -c android.emu.debug",
    "ci:test:e2e:android": "detox test -c android.emu.debug -l verbose --cleanup",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write \"**/*.js\"",
    "format:json": "prettier --write \"**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"**/*.ts{,x}\"",
    "lint": "eslint index.js app storybook test --fix --ext .js,.ts,.tsx && yarn format",
    "patch": "patch-package",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "test": "jest --watch",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "mst-gql": "mst-gql",
    "bootsplash": "yarn react-native generate-bootsplash assets/bootsplash/splash.png --background-color=FFF --logo-width=100 --assets-path=assets/bootsplash --flavor=main"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.2",
    "@motify/skeleton": "^0.11.0",
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^6.0.1",
    "@react-native-firebase/admob": "^11.4.1",
    "@react-native-firebase/app": "^11.4.1",
    "@react-native-firebase/auth": "^11.4.1",
    "@react-native-picker/picker": "^1.16.6",
    "@react-navigation/material-bottom-tabs": "^5.3.15",
    "@react-navigation/material-top-tabs": "^5.3.15",
    "@react-navigation/native": "5.8.10",
    "@react-navigation/stack": "5.12.8",
    "@sentry/react-native": "^2.6.2",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.8",
    "@types/uuid": "^8.3.1",
    "@unimodules/core": "6.0.0",
    "apisauce": "2.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expo-linear-gradient": "^9.2.0",
    "expo-localization": "9.1.0",
    "graphql-request": "^3.4.0",
    "i18n-js": "3.8.0",
    "jest-cli": "^26.6.3",
    "mobx": "6.0.4",
    "mobx-react-lite": "3.1.6",
    "mobx-state-tree": "4.0.2",
    "moti": "^0.11.0",
    "mst-gql": "^0.14.0",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-bootsplash": "^3.2.4",
    "react-native-bouncy-checkbox": "^2.1.2",
    "react-native-collapsible": "^1.5.3",
    "react-native-deck-swiper": "^2.0.5",
    "react-native-dotenv": "^2.5.5",
    "react-native-fbsdk-next": "^4.3.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "^1.7.0",
    "react-native-keychain": "6.2.0",
    "react-native-reanimated": "^2.2.0",
    "react-native-redash": "^16.1.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^3.4.0",
    "react-native-share": "^7.0.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1-0",
    "react-native-tab-view": "^2.16.0",
    "react-native-toast-message": "^1.4.9",
    "react-native-ui-lib": "^5.29.0",
    "react-native-unimodules": "0.11.0",
    "react-native-vector-icons": "^8.1.0",
    "reactotron-mst": "3.1.3",
    "reactotron-react-native": "5.0.0",
    "uuid": "^8.3.2",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-optional-catch-binding": "7.12.1",
    "@sentry/webpack-plugin": "^1.15.0",
    "@storybook/addon-storyshots": "6.1.10",
    "@storybook/react-native": "5.3.23",
    "@storybook/react-native-server": "5.3.23",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^8.0.0-rc.0",
    "@types/faker": "^5.5.3",
    "@types/i18n-js": "3.0.3",
    "@types/jest": "26.0.19",
    "@types/ramda": "0.27.32",
    "@types/react": "16.14.0",
    "@types/react-native": "0.63.40",
    "@types/react-test-renderer": "16.9.4",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "bufferutil": "4.0.2",
    "canvas": "2.6.1",
    "detox": "^18.13.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-native": "3.10.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-circus": "25.5.4",
    "jest-expo": "40.0.1",
    "jetifier": "1.6.6",
    "mocha": "8.2.1",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "2.2.1",
    "react-devtools-core": "4.10.1",
    "react-dom": "16.14.0",
    "react-native-web": "0.13.18",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "17.0.1",
    "rimraf": "3.0.2",
    "solidarity": "2.3.1",
    "typescript": "4.1.3",
    "utf-8-validate": "5.0.3"
  },
  "jest": {
    "preset": "jest-expo",
    "clearMocks": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/test/setup.ts"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation|@storybook|@react-native-community|expo-localization|@unimodules)"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "detox": {
    "test-runner": "jest",
    "artifacts": {
      "rootDir": ".artifacts",
      "plugins": {
        "instruments": {
          "enabled": false
        },
        "log": {
          "enabled": true
        },
        "uiHierarchy": "enabled",
        "screenshot": {
          "enabled": true,
          "shouldTakeAutomaticSnapshots": true,
          "keepOnlyFailedTestsArtifacts": true,
          "takeWhen": {
            "testStart": true,
            "testDone": true
          }
        },
        "video": {
          "android": {
            "bitRate": 4000000
          },
          "simulator": {
            "codec": "hevc"
          }
        }
      }
    },
    "devices": {
      "emulator": {
        "type": "android.emulator",
        "device": {
          "avdName": "android_29_avd"
        }
      },
      "simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 12 Pro Max"
        }
      }
    },
    "apps": {
      "android.debug": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug app:assembleAndroidTest -DtestBuildType=debug && cd .."
      },
      "android.release": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease app:assembleAndroidTest -DtestBuildType=release && cd .."
      },
      "ios.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/punchline.app",
        "type": "ios.app",
        "build": "xcodebuild -workspace ios/punchline.xcworkspace -scheme punchline -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 12'"
      },
      "ios.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/punchline.app",
        "type": "ios.app",
        "build": "xcodebuild -workspace ios/punchline.xcworkspace -scheme punchline -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 12'"
      }
    },
    "configurations": {
      "android.emu.debug": {
        "app": "android.debug",
        "device": "emulator"
      },
      "android.emu.release": {
        "app": "android.release",
        "device": "emulator"
      },
      "ios.sim.debug": {
        "device": "simulator",
        "app": "ios.debug"
      },
      "ios.sim.release": {
        "device": "simulator",
        "app": "ios.release"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard"
    ],
    "plugins": [
      "react",
      "react-native",
      "@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "indent": "off",
      "@typescript-eslint/member-delimiter-style": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/no-unnecessary-condition": [
        2
      ],
      "comma-dangle": 0,
      "multiline-ternary": 0,
      "no-undef": 0,
      "no-unused-vars": 0,
      "no-use-before-define": "off",
      "quotes": 0,
      "react-native/no-raw-text": 0,
      "react/no-unescaped-entities": 0,
      "react/prop-types": "off",
      "space-before-function-paren": 0,
      "no-restricted-imports": [
        "error",
        {
          "paths": [
            {
              "name": "react-native",
              "importNames": [
                "View",
                "Text",
                "Image",
                "Button"
              ],
              "message": "Prefer react-native-ui-lib imports instead"
            }
          ]
        }
      ]
    }
  }
}
