{
  "main": "src/App.tsx",
  "scripts": {
    "android": "concurrently  \"npm run run:android-emulator\" \"sleep 2 && npm run run:android\"  \"npm run lint:watch\"",
    "run:android": "npx react-native run-android",
    "run:android-emulator": "emulator @Pixel_3a_API_29 -port 9090 -dns-server 8.8.8.8",
    "atest": "sleep 5 && echo 'test'",
    "ios": "npx react-native run-ios && npm run lint:watch",
    "web": "expo start --web",
    "start": "react-native start",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "setup": "npm install && npm run validate",
    "validate": "npm run lint && npm run test",
    "lint": "./node_modules/.bin/eslint --fix --ignore-path .gitignore src/",
    "lint:watch": "./node_modules/.bin/esw ./src/**.* -w --clear",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts|tsx|js|json|css|html|md)\""
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^8.2.0",
    "@react-native-firebase/auth": "^8.2.0",
    "@react-native-firebase/database": "^7.3.2",
    "@react-native-firebase/firestore": "^7.4.3",
    "@react-navigation/bottom-tabs": "^5.7.3",
    "@react-navigation/native": "^5.7.2",
    "@react-navigation/stack": "^5.8.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "expo": "~38.0.8",
    "expo-splash-screen": "^0.3.1",
    "expo-status-bar": "^1.0.0",
    "expo-updates": "~0.2.8",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "~0.62.2",
    "react-native-elements": "^2.1.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-reanimated": "^1.10.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-unimodules": "^0.10.1",
    "react-native-vector-icons": "^7.0.0",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/jest-native": "^3.1.0",
    "@testing-library/react-native": "^7.0.1",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^26.0.3",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.61.23",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "~8.2.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jest-dom": "^3.0.1",
    "eslint-watch": "^7.0.0",
    "husky": "^4.2.5",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.1.0",
    "jest-expo": "~38.0.0",
    "jest-runner-eslint": "^0.10.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "reactotron-react-native": "^5.0.0",
    "typescript": "~3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(ts|tsx|js|css|html|md)": [
      "prettier",
      "eslint --fix --ignore-path .gitignore",
      "jest --findRelatedTests"
    ]
  },
  "private": true
}
